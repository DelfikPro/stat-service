{
  "name": "stats-service",
  "main": "dist/server",
  "_moduleAliases": {
    "@": "src"
  },
  "paths": {
    "@": "src"
  },
  "scripts": {
    "fmt": "eslint --fix .",
    "dev": "NODE_ENV=development ts-node src/",
