    "build": "tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./build"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "logform": "^2.2.0",
    "module-alias": "^2.2.2",
    "mongodb": "^3.6.4",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "ws": "^7.4.1"
  },
  "devDependencies": {
    "@types/mongodb": "^3.6.8",
    "@types/node": "^14.14.14",
    "@types/uuid": "^8.3.0",
    "@types/ws": "^7.4.0",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "eslint": "^7.22.0",
    "eslint-plugin-prettier": "^3.3.1",
    "prettier": "^2.2.1",
    "tscpaths": "^0.0.9",
    "typescript": "^4.1.3"
  }
